<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Services" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="progressbar1">
        <item name="colorAccent">@color/md_green_600</item>
    </style>

    <style name="progressbar2">
        <item name="colorAccent">@color/teal_700</item>
    </style>

    <style name="progressbar3">
        <item name="colorAccent">@color/violet</item>
    </style>

    <style name="progressbar4">
        <item name="colorAccent">@color/md_grey_700</item>
    </style>

    <style name="progressbar5">
        <item name="colorAccent">@color/md_red_600</item>
    </style>
</resources>